{
    "name": "integrity/dinghy",
    "description": "Docker files for running a basic php application development environment with testing, codelinting and documentation tools.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Integrity\\Dinghy\\": "src/"
        }
    },
    "require": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "bin": [
        "bin/docs/index.php",
        "bin/docker-compose.yml",
        "bin/Dockerfile",
        "bin/phpdoc.xml",
        "bin/phpunit.xml"
    ]
}
